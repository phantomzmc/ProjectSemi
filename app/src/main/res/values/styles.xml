<resources>

    <!-- Base application theme. -->


    <style name="HeaderText1" parent="@android:style/TextAppearance.Medium">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/blue_sky</item>
        <item name="android:typeface">monospace</item>
        <item name="android:textStyle">bold</item>

    </style>



    <style name="Texttint">
        <item name="android:textColorHint">@color/white</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="BottonColor1">
        <item name="android:background">@color/green_apple</item>
    </style>

    <style name="textback">
        <item name="android:textColor">@color/back</item>
    </style>


    <!-- Base application theme. -->
    <style name="AppTheme" parent="android:Theme.Material.Light">
        <!-- Customize your theme here. -->
        <item name="android:actionBarStyle">@style/MyActionBar</item>
        <!-- Support library compatibility -->
        <item name="actionBarStyle">@style/MyActionBar</item>
    </style>

    <!-- ActionBar styles -->
    <style name="MyActionBar"
        parent="@style/Widget.AppCompat.Light.ActionBar.Solid.Inverse">
        <item name="android:background">@color/gray</item>

        <!-- Support library compatibility -->

    </style>





</resources>
